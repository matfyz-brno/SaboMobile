{
  "name": "bmi-calculator-cypress-tests",
  "version": "1.0.0",
  "description": "Cypress tests for BMI Calculator application",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:edge": "cypress run --browser edge",
    "cy:run:headless": "cypress run --headless",
    "cy:run:headed": "cypress run --headed",
    "cy:test": "cypress run --spec 'cypress/e2e/**/*.cy.js'",
    "cy:test:chrome": "cypress run --browser chrome --spec 'cypress/e2e/**/*.cy.js'",
    "cy:test:firefox": "cypress run --browser firefox --spec 'cypress/e2e/**/*.cy.js'",
    "cy:install": "cypress install",
    "cy:verify": "cypress verify",
    "test": "npm run cy:run",
    "test:headed": "npm run cy:run:headed",
    "test:chrome": "npm run cy:run:chrome",
    "test:firefox": "npm run cy:run:firefox"
  },
  "keywords": [
    "cypress",
    "testing",
    "e2e",
    "bmi-calculator",
    "javascript",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.13.0"
  }
}